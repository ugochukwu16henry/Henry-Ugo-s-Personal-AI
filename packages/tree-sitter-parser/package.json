{
  "name": "@henry-ai/tree-sitter-parser",
  "version": "1.0.0",
  "description": "Tree-sitter integration for AST parsing and symbol extraction",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "setup-wasm": "node scripts/download-wasm.mjs"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
